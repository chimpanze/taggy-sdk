{
  "name": "@chimpanze/taggy-sdk",
  "version": "0.2.2",
  "description": "TypeScript SDK for Taggy API",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chimpanze/taggy-sdk.git"
  },
  "homepage": "https://github.com/chimpanze/taggy-sdk",
  "bugs": {
    "url": "https://github.com/chimpanze/taggy-sdk/issues"
  },
  "keywords": [
    "taggy",
    "sdk",
    "api",
    "client",
    "typescript"
  ],
  "author": "Taggy Team",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "generate:types": "openapi-typescript ./openapi.yaml -o ./src/types/generated.ts",
    "docs": "typedoc src/index.ts",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "openapi-typescript-fetch": "^2.2.1",
    "zod": "^4.0.14"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^11.1.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^9.2.6",
    "@semantic-release/npm": "^11.0.2",
    "@semantic-release/release-notes-generator": "^12.1.0",
    "@types/node": "^24.1.0",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.32.0",
    "openapi-typescript": "^7.8.0",
    "prettier": "^3.6.2",
    "semantic-release": "^22.0.12",
    "terser": "^5.43.1",
    "typedoc": "^0.28.9",
    "typescript": "^5.8.3",
    "vite": "^7.0.6",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.4"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com/"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ]
}
